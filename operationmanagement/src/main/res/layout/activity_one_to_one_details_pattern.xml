<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".ui.activity.edu.OneToOneDetailsActivity"
        android:orientation="vertical"
        android:background="@color/white"
        android:layout_marginLeft="@dimen/marginLeft_10"
        android:layout_marginRight="@dimen/marginRight_10">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/layout_height_55"
            android:orientation="horizontal"
            android:background="@color/white"
            android:gravity="center_vertical">

        <TextView
                android:id="@+id/tv_memberName"
                android:text="TextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.7"
                android:gravity="center|left"/>
        <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:gravity="center|right">

            <TextView
                    android:id="@+id/tv_memberTimetable"
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
        </LinearLayout>
        <TextView
                android:layout_width="1dp"
                android:layout_height="@dimen/layout_height_35"
                android:background="@color/background_f4f4f4"/>
        <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:background="@color/white">

            <TextView
                    android:id="@+id/tv_attendance"
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>
    <include layout="@layout/view_line_onedp"/>
</LinearLayout>
